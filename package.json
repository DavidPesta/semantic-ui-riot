{
  "name": "semantic-ui-riot",
  "version": "0.0.1",
  "description": "Semantic UI for Riot",
  "main": "dist/semantic-ui-riot.min.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "riot": "riot tags dist --type es6",
    "minify": "uglifyjs --output dist/semantic-ui-riot.js dist/**/*",
    "uglify": "uglifyjs --compress --mangle --screw-ie8 --comment false --output dist/semantic-ui-riot.min.js dist/**/*",
    "dev": "npm run riot && npm run minify && npm run uglify"
  },
  "keywords": [
    "riotjs",
    "riot",
    "semantic",
    "semanticui"
  ],
  "author": "black-trooper",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/black-trooper/semantic-ui-riot/issues"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.25.0",
    "babel-preset-es2015-riot": "^1.1.0",
    "riot": "^3.6.1",
    "uglify-js": "^3.0.25"
  }
}
